{
  "name": "mt-tacoshop",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^5",
    "@sap/xsenv": "^3.1.1",
    "@sap/xssec": "^3.2.12",
    "body-parser": "^1.19.1",
    "cds-pg": "^0.1.22",
    "cfenv": "^1.2.4",
    "express": "^4",
    "passport": "^0.5.2"
  },
  "devDependencies": {
    "cds-dbm": "github:austinkloske22/cds-dbm#main",
    "sqlite3": "^5.0.2"
  },
  "scripts": {
    "start": "node srv/server.js",
    "deploy:sqlite": "cds deploy -2 sqlite::db:",
    "docker:start:pg": "docker-compose -f docker-compose.yml up -d",
    "deploy:pg": "node_modules/cds-dbm/dist/cli.js deploy --create-db",
    "deploy:pg:load": "node_modules/cds-dbm/dist/cli.js deploy --load-via delta"
  },
  "cds": {
    "requires": {
      "db": {
        "kind": "database"
      },
      "database": {
        "dialect": "plain",
        "impl": "cds-pg",
        "model": [
          "srv"
        ]
      }
    },
    "migrations": {
      "db": {
        "multitenant": true,
        "schema": {
          "default": "public",
          "clone": "_cdsdbm_clone",
          "schema": "public",
          "reference": "_cdsdbm_ref",
          "tenants": [
            "_c21a0f01a094445a97616901eda6b86a",
            "_39b01c859d9742838c84ca8b0b7680bb",
            "_699826aaa971415b8556c5ebb080232c"
          ]
        },
        "deploy": {
          "tmpFile": "tmp/_autodeploy.json",
          "undeployFile": "db/undeploy.json"
        }
      }
    }
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "es2020": true,
      "node": true,
      "jest": true,
      "mocha": true
    },
    "globals": {
      "SELECT": true,
      "INSERT": true,
      "UPDATE": true,
      "DELETE": true,
      "CREATE": true,
      "DROP": true,
      "CDL": true,
      "CQL": true,
      "CXL": true,
      "cds": true
    },
    "rules": {
      "no-console": "off",
      "require-atomic-updates": "off"
    }
  }
}
